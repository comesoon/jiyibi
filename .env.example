# .env.example
# This file is a template for the .env file.
# Copy this file to .env and fill in the values for your environment.

# MongoDB connection string
# Format: mongodb://[username:password@]host1[:port1]/database
MONGO_URI=mongodb://jiyibi_user:your_app_password@localhost:27017/jiyibi

# Node.js server port
PORT=5000

# JWT secret key for signing tokens (use a long, random string)
# Generate one with: openssl rand -base64 32
JWT_SECRET=your_super_long_and_random_jwt_secret

# Public URL of the frontend application (for CORS)
FRONTEND_URL=http://localhost:3000
